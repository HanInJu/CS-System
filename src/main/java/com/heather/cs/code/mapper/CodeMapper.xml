<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heather.cs.code.mapper.CodeMapper">

    <select id="selectGroupCode" parameterType="String" resultType="String">
        SELECT group_code
        FROM common_code
        WHERE code = #{state}
    </select>

</mapper>